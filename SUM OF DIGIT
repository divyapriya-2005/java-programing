import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        if (n < 100) {
            System.out.println("Invalid Input");
            return;
        }

        int sum = 0;
        int temp = n;
        while (temp > 0) {
            int digit = temp % 10; 
            sum += digit;
            temp = temp / 10;      
        }

        System.out.println("Sum of digit is " + sum);
    }
}
